{
    "manifest_version": 3,
    "name": "To-Do List BY Samarth",
    "version": "1.0",
    "description": "A simple to-do list Chrome extension.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  